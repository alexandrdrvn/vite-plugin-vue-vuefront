{
    "name": "vite-plugin-vue-vuefront",
    "version": "0.0.2",
    "description": "vite-plugin-vue-vuefront",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "templates/**/*",
        "installComponents.js"
    ],
    "scripts": {
        "dev": "tsc -p . -w --incremental",
        "build": "rimraf dist && run-s build-bundle build-types",
        "build-bundle": "nodemon -e ts -x esbuild src/index.ts --bundle --platform=node --target=node12 --external:@vue/compiler-sfc --outfile=dist/index.js",
        "build-types": "tsc -p . --emitDeclarationOnly --outDir temp && api-extractor run && rimraf temp",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package plugin-vue",
        "release": "node ../../scripts/release.js",
        "yalc-watch": "nodemon -e vue,js,scss,json -x yalc push"
    },
    "keywords": [
        "vite-plugin",
        "vuefront"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/estree": "0.0.50",
        "@types/node": "^16.4.11",
        "@types/stylelint": "^13.13.1",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "eslint": "^7.32.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "nodemon": "^2.0.12",
        "typescript": "^4.3.5",
        "vite": "^2.4.4"
    },
    "dependencies": {
        "@rollup/pluginutils": "^4.1.1",
        "@types/dotenv": "^8.2.0",
        "@types/lodash": "^4.14.172",
        "@vue/compiler-sfc": "^3.1.5",
        "apollo-boost": "^0.4.9",
        "assign-deep": "^1.0.1",
        "cookie-universal": "^2.1.5",
        "dotenv": "^10.0.0",
        "isomorphic-fetch": "^3.0.0",
        "jscodeshift": "^0.13.0",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "querystring": "^0.2.1",
        "rimraf": "^3.0.2",
        "vue-i18n": "8.25.0",
        "vue-router": "^3.5.2",
        "vue-template-compiler": "^2.6.14",
        "vuefront": "^0.3.72"
    },
    "y": {
        "rollup": "^2.38.5",
        "vite": "^2.4.4",
        "vuefront": "^0.3.72"
    }
}
